<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_with scope: :session, url: login_path do |form| %>
    <%= form.label :email %>
    <%= form.text_field :email %><br>
    <br>
    <%= form.label :password %>
    <%= form.password_field :password %><br>
    <br>
    <%= form.submit 'Login' %>

<% end %>