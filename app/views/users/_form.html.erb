<%= form_with model: @user do |form| %>
    <%= form.label :first_name %>
    <%= form.text_field :first_name %><br>
    <br>
    <%= form.label :last_name %>
    <%= form.text_field :last_name %><br>
    <br>
    <%= form.label :email %>
    <%= form.text_field :email %><br>
    <br>
    <%= form.label :password %>
    <%= form.password_field :password %><br>
    <br>
    <%= form.submit %>
<% end %>